{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ES2020",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "allowImportingTsExtensions": true,
        "strict": true,
        //    "rootDir": "./src",
        "exclude": ["node_modules"]
    }
}
